{
    "manifest_version": 3,
    "name": "Bing Quick Search",
    "description": "An extension that completes the search requirements for the rewards.",
    "version": "1.1",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "permissions": ["tabs"],
    "action": {
        "default_icon": { "16": "icon.png" },
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "service-worker.js"
    }
}
